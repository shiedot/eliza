{
  "version": 2,
  "buildCommand": "bun run build",
  "installCommand": "bun install --frozen-lockfile",
  "framework": null,
  "outputDirectory": "packages/server/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/server/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "packages/server/src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
